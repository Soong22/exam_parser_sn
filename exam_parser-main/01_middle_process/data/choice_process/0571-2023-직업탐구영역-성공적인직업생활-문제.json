    "bbox": [
      86,
      540,
      132,
      554
    ],
    "page_idx": 2
  },
  {
    "type": "text",
    "text": "다음은",
    "bbox": [
      132,
      540,
      167,
      555
    ],
    "page_idx": 2
  },
  {
    "type": "inline_equation",
    "content": "\\bigcirc \\bigcirc",
    "bbox": [
      168,
