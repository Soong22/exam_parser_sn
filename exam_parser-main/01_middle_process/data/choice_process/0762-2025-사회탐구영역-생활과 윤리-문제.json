    "table_body": "<table><tr><td rowspan=1 colspan=1>수험번호</td></tr></table>",
    "table_caption": [],
    "table_footnote": [],
    "image_path": "abce6ee656efe63c61b773eec0f24396053a2415c89f3c11f4c77ddcae3cd87d.jpg",
