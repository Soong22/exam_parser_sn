    "page_idx": 3
  },
  {
    "type": "text",
    "text": "갖는 상대적 특징을 그림의",
    "bbox": [
      446,
      637,
      579,
      654
    ],
    "page_idx": 3
  },
  {
    "type": "inline_equation",
