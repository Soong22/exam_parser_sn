    "bbox": [
      577,
      973,
      622,
      991
    ],
    "page_idx": 0,
    "mathpix": false
  },
  {
    "type": "text",
    "text": "7.",
    "bbox": [
      456,
      991,
      471,
      1005
    ],
    "page_idx": 0,
